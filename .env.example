# Database
DATABASE_URL="postgresql://user:password@host:port/database"

# NextAuth
NEXTAUTH_URL=http://localhost:3000
NEXTAUTH_SECRET=your-secret-key-here

# Google OAuth
GOOGLE_CLIENT_ID=your-google-client-id
GOOGLE_CLIENT_SECRET=your-google-client-secret

# ScreenshotOne
SCREENSHOTONE_ACCESS_KEY=your-access-key
SCREENSHOTONE_SECRET_KEY=your-secret-key

# Stripe (dla subskrypcji premium)
# Pobierz z: https://dashboard.stripe.com/apikeys
STRIPE_SECRET_KEY=sk_test_your_stripe_secret_key
# Pobierz z: https://dashboard.stripe.com/webhooks (po utworzeniu webhook endpoint)
STRIPE_WEBHOOK_SECRET=whsec_your_webhook_secret

# Cloudinary (dla uploadu logo)
# Pobierz z: https://cloudinary.com/console
CLOUDINARY_CLOUD_NAME=your-cloud-name
CLOUDINARY_API_KEY=your-api-key
CLOUDINARY_API_SECRET=your-api-secret
